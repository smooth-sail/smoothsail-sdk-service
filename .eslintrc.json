{
  "env": {
    "es2021": true,
    "node": true,
    "jest/globals": true
  },
  "extends": ["eslint:recommended", "plugin:import/recommended"],
  "plugins": ["import"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "import/no-commonjs": 1
  }
}
